	Issn = {1841429X},
	Journal = {Studies in Informatics and Control},
	Keywords = {Cement grinding circuit,Feed-grindability,Generalized Predictive Controller (GPC)},
	Number = {1},
	Pages = {29--38},
	Title = {{Generalized predictive controller for ball mill grinding circuit in the presence of feed-grindability variations}},
	Volume = {25},
	Year = {2016}}

